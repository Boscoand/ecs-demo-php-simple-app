{
    "executionRoleArn": "arn:aws:iam::account_ID:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "simple-app",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80
                }
            ],
            "environment": {
                "name": "Tag",
                "value": ""
            },
            "entryPoint": [
                "/usr/sbin/apache2",
                "-D",
                "FOREGROUND"
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}